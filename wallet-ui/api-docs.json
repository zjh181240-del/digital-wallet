    "components": {
        "schemas": {
            "TTransactionBo": {
                "required": [
                    "amount",
                    "chain",
                    "createdAt",
                    "fromAddress",
                    "id",
                    "status",
                    "toAddress",
                    "token",
                    "txid",
                    "updatedAt",
                    "userId"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "description": "主键",
                        "format": "int64"
                    },
                    "userId": {
                        "type": "integer",
                        "description": "发起交易的用户ID",
                        "format": "int64"
                    },
                    "chain": {
                        "type": "string",
                        "description": "交易所在的区块链网络类型"
                    },
                    "token": {
                        "type": "string",
                        "description": "交易的代币类型，native代表链原生代币（如MATIC/BNB），usdt代表该链上的USDT"
                    },
                    "fromAddress": {
                        "type": "string",
                        "description": "交易发送方的区块链地址"
                    },
                    "toAddress": {
                        "type": "string",
                        "description": "交易接收方的区块链地址"
                    },
